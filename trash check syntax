string checkSyntax(vector<vector<string>> input){
  string errorline = "-1";
  for(vector<string> line : input){
    for(int k = 0; k < line.size()-1; k++){
      //rem: no errors can possibly be thrown
      if(line[k] == "rem"){k = line.size()-1;}
      /* let and input must have variables in the next word.
       * let must have "=" then an expression -- not done yet.
       * and nothing after */
      else if(line[k] == "let" || line[k] == "input"){
        if(!isVariable(line[k+1])){errorline = line[0];}
      }
      //goto must have number in next line and nothing after that
      else if(line[k] == "goto"){
        if(!isNumber(line[k+1])){errorline = line[0];}
      }
      //print must have either a number or variable and nothing after
      else if(line[k] == "print"){
        if(!isVariable(line[k+1]) && !isNumber(line[k+1])){errorline = line[0];}
      }
      else if(line[k] == "if"){
        
      }
      /* no checking is done on if & end. -- not done yet. 
       * if: expression, comparator, expression, command not rem
       * where expression can be a variale or literal
       * end: nothing after*/
       //sweeping up the rest: 
      else if( !isCommand(line[k]) && !isNumber(line[k]) && !isVariable(line[k]) ){
        cout<<line[k]<<" ";
        errorline = line[0];}
    }
  }
  return errorline;
}